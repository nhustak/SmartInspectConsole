{
  "name": "smartinspect-js",
  "version": "1.0.0",
  "description": "SmartInspect logging client for browser-based JavaScript/TypeScript applications",
  "main": "dist/smartinspect.umd.js",
  "module": "dist/smartinspect.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/smartinspect.esm.js",
      "require": "./dist/smartinspect.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run bundle",
    "bundle": "node scripts/bundle.js",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "smartinspect",
    "logging",
    "debug",
    "browser",
    "typescript",
    "websocket",
    "real-time"
  ],
  "author": "Nick Hustak",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhustak/SmartInspectConsole.git",
    "directory": "smartinspect-js"
  },
  "bugs": {
    "url": "https://github.com/nhustak/SmartInspectConsole/issues"
  },
  "homepage": "https://github.com/nhustak/SmartInspectConsole/tree/master/smartinspect-js#readme",
  "devDependencies": {
    "typescript": "^5.0.0",
    "esbuild": "^0.20.0"
  }
}
