{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Relay": {
    "ConsoleHost": "localhost",
    "ConsolePort": 4229,
    "BufferSize": 10000,
    "ReconnectDelayMs": 5000,
    "MaxReconnectAttempts": 0,
    "RateLimit": {
      "RequestsPerMinute": 1000,
      "BurstSize": 100
    },
    "ApiKeys": {
      "Enabled": false,
      "Keys": []
    }
  }
}
